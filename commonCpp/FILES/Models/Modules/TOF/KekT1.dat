// FILES/Models/Modules/TOF/KekT1.dat
//
// Definition of the T1 scintillation counter in the KEK test beam
//
// M.Ellis 22nd May 2006

Module KekT1
{
  Volume Box
  Dimensions 20 5.0 2.0 cm
  PropertyString Detector TOF
  PropertyDouble PhotoElectronsResolution 9.7
  PropertyInt Station 1
  PropertyInt numPlanes 1
  PropertyInt Plane 1
  PropertyInt numSlabs 1
  PropertyInt Slab 1
  PropertyInt numPMTs 1
  PropertyHep3Vector Pmt1Pos 10 0.0 0.0 cm
  PropertyHep3Vector Pmt2Pos -10 0.0 0.0 cm
  PropertyDouble RedColour 1.0
  PropertyDouble BlueColour 1.0
  Module TOF/KekT1SciFi.dat
  {
     Position 0.0 0.0 0.0 cm
     Rotation 0.0 0.0 0.0 degree
  }
}
